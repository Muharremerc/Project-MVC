         function addCompanyAndUser() {
             var data = {
                 cname: $('#CName').val(),
                 name: $('#Name').val(),
                 surname: $('#Surname').val(),
                 username: $('#Username').val(),
                 password: $('#Password').val()

             };

             $.ajax
                 ({
                     url: "/Company/AddCompany",
                     dataType: "json",
                     type: "POST",
                     contentType: 'application/json',
                     data: JSON.stringify(data),
                     processData: false,
                     success: window.location.assign("/Login/Index")
                 }
                 );
         }

    </script>

