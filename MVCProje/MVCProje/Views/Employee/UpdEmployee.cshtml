@model ProjeDB.Employee
@using  ProjeDB
@{
    ViewBag.Title = "UpdEmployee";
    if (Session["Check"].ToString() != "Emp")
    {

        Response.Redirect("~/Login/Index");

    }
    var a = Session["emp"] as Employee ;
    var b = Model;
    if ( a.Id!= b.Id)
    {
        Response.Redirect("~/Login/Index");
    }
}




<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Add New Employee
            </header>
            <div class="panel-body">
                <form class="form-horizontal" id="default">
                    <fieldset title="Step 2" class="step" id="default-step-1" style="display: block;">
                        <legend> </legend>
                        <div class="form-group">


                            <input type="hidden" class="form-control" cols="20" rows="15" id="id" value="@Model.Id" />


                            <label class="col-lg-2 control-label">Name</label>
                            <div class="col-lg-10" style="padding-bottom:15px">
                                <input type="text" class="form-control" cols="20" rows="15" id="Name" />
                            </div>

                            <label class="col-lg-2 control-label">Surname</label>
                            <div class="col-lg-10" style="padding-bottom:15px">
                                <input type="text" class="form-control" cols="20" rows="15" id="Surname" />
                            </div>

                            <label class="col-lg-2 control-label">Username</label>
                            <div class="col-lg-10" style="padding-bottom:15px">
                                <input type="text" class="form-control" cols="20" rows="15" id="Username" />
                            </div>

                            <label class="col-lg-2 control-label">Password</label>
                            <div class="col-lg-10" style="padding-bottom:15px">
                                <input type="text" class="form-control" cols="20" rows="15" id="Password" />
                            </div>


                        </div>

                        <p id="default-buttons-1" class="default-buttons">

                            <a id="button_2" href="javascript:upd();" class="button-back  btn btn-info">Upd</a>
                        </p>
                    </fieldset>
                </form>
            </div>
        </section>
    </div>
</div>


@section script
{
    <script>

        function upd() {


                var data = {

                    id: $('#id').val(),
                    name: $('#Name').val(),
                    surname: $('#Surname').val(),
                    username: $('#Username').val(),
                    password: $('#Password').val()

                 }

            $.ajax
                ({
                    url: "/Employee/upde",
                    dataType: 'JSON',
                    type: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify(data),
                    processData: false,
                    success: function (data) {
                        alert("Güncellendi");
                        success: window.location.assign("/Login/LogOut");
                    },
                    error: function (req, status, err) {
                        console.log('something went wrong', status, err);
                    }


                });

        }


        function fill(_id) {
            if (_id != 0) {

                var data = { id: _id }

                $.ajax
                    ({
                        url: "/Employee/fillemp",
                        dataType: 'JSON',
                        type: 'POST',
                        contentType: 'application/json',
                        data: JSON.stringify(data),
                        processData: false,
                        success: function (data) {

                            $("#Name").val(data["Name"]);
                            $("#Surname").val(data["Surname"]);
                            $("#Password").val(data["Password"]);
                            $("#Username").val(data["UserName"]);
                            $("#id").val(data["Id"]);

                        },
                        error: function (req, status, err) {
                            console.log('something went wrong', status, err);
                        }


                    });
            }
        }

        fill(@Model.Id)
    </script>






