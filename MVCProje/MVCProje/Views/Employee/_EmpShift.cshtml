@using ProjeDB
@model List<EmployeeShift>
<div class="row">

    <section class="panel">
        <header class="panel-heading">
            <h1>Shifth</h1>
        </header>
        <div class="panel-body">
            <div class="adv-table">
                <table border="0" class="display table table-bordered" id="tableList">
                    <thead>
                        <tr>
                            
                            <th>Sname</th>
                            <th>start</th>
                            <th>Finish</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {

                            <tr>
                             <td>   @item.Shift.Name</td>
                                <td>   @item.Start</td>
                                <td>  @item.Finish</td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </section>
</div>

